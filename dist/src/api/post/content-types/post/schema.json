{
    "kind": "collectionType",
    "collectionName": "posts",
    "info": {
        "singularName": "post",
        "pluralName": "posts",
        "displayName": "Post",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string"
        },
        "content": {
            "type": "text"
        },
        "photo": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        },
        "approved": {
            "type": "boolean",
            "default": false
        },
        "user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user"
        },
        "post_comments": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::post-comment.post-comment",
            "mappedBy": "post"
        },
        "post_reacts": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::post-react.post-react",
            "mappedBy": "post"
        },
        "user_savings": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "plugin::users-permissions.user",
            "mappedBy": "post_saveds"
        }
    }
}
